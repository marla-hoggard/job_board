<%= form_for @job do |f| %>
  <div>
    <%= f.label :title %><br/>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %><br/>
    <%= f.text_area :description, size: '60x6' %>
  </div>
  <div>
    <%= f.submit %>
    <button><%= link_to "Back", jobs_path %></button>
  </div>
<% end %>